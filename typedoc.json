{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "src/index.ts"
  ],
  "out": "docs",
  "theme": "default",
  "name": "Zod RPC - Type-Safe RPC Library",
  "readme": "README.md",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "hideGenerator": true,
  "sort": ["source-order"],
  "kindSortOrder": [
    "Function",
    "Class",
    "Interface", 
    "TypeAlias",
    "Variable",
    "Enum"
  ],
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Services",
    "Client", 
    "Server",
    "Transports",
    "Types",
    "*"
  ],
  "gitRevision": "main",
  "gitRemote": "origin",
  "sourceLinkTemplate": "https://github.com/xtr-dev/zod-rpc/blob/{gitRevision}/{path}#L{line}",
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "searchInComments": true
}